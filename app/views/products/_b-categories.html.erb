<section class="b-categories">
	<div class="col-1-2">
	<div class="module">
		<h4>Дизайнеры</h4>
		<ul>
			<% designers.each do |designer| %>
				<li><%= link_to designer.name, type_designer_url(type, designer)  %></li>
			<% end %>    
		</ul>
	</div>
</div>
<div class="col-1-2">
	<div class="module">
		<h4>Фасоны</h4>
		<ul>
			<% styles.each do |style| %>
				<li><%= link_to style.name, type_style_url(type, style)  %></li>
			<% end %>    
		</ul>
	</div>
</div>
</section>