<!DOCTYPE html>
<html>
<head>
  <title>Dress</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

	<% flash.each do |name, msg| %>
	  <%= content_tag :div, msg, :id => "flash-#{name}", :class => "flash-message" %>
	<% end %>
	<%= link_to("Log out", log_out_path) if session[:log_in] %>
<%= yield %>

</body>
</html>
